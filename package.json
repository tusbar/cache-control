{
  "name": "@tusbar/cache-control",
  "public": true,
  "license": "MIT",
  "version": "1.0.2",
  "author": "Bertrand Marron <bertrand.marron@gmail.com>",
  "description": "Format and parse HTTP Cache-Control header",
  "repository": "https://github.com/tusbar/cache-control",
  "keywords": [
    "cache-control",
    "http",
    "rfc7234"
  ],
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "source": "./src/index.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "test": "jest",
    "check:ts": "tsc --noEmit",
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "devDependencies": {
    "@bizon/semantic-release-config": "^3.0.2",
    "@jest/types": "^30.0.1",
    "@swc/core": "^1.11.29",
    "@swc/jest": "^0.2.38",
    "@tsconfig/node22": "^22.0.0",
    "@types/jest": "^29.5.14",
    "eslint-config-xo-bizon": "^3.0.3",
    "jest": "^29.7.0",
    "semantic-release": "^24.2.5",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "typescript": "^5.7.2",
    "xo": "^0.59.3"
  },
  "xo": {
    "extends": "xo-bizon",
    "semicolon": false,
    "space": 2,
    "prettier": true
  },
  "prettier": {
    "printWidth": 100
  },
  "release": {
    "extends": "@bizon/semantic-release-config"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "cjs",
      "esm"
    ],
    "sourcemap": true,
    "dts": true
  },
  "packageManager": "pnpm@10.8.1+sha512.c50088ba998c67b8ca8c99df8a5e02fd2ae2e2b29aaf238feaa9e124248d3f48f9fb6db2424949ff901cffbb5e0f0cc1ad6aedb602cd29450751d11c35023677"
}
